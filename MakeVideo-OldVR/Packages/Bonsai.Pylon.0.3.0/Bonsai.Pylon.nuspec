<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
  <metadata>
    <id>Bonsai.Pylon</id>
    <version>0.3.0</version>
    <title>Bonsai - Pylon Library</title>
    <authors>Gonçalo Lopes</authors>
