              <scr:Expression>new(
double.Parse(it[0]) * Math.PI / 180 as Orientation,
double.Parse(it[1]) as LocationX,
double.Parse(it[2]) as LocationY,
double.Parse(it[3]) as Delay)</scr:Expression>
